<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
            name="listener"
            type="android.view.View.OnClickListener" />
    </data>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">


        <heyong.handong.framework.widget.TopBar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:centerText="退款详情" />

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:background="#F6F6F6" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_85"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_30"
                        android:orientation="horizontal">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="提现状态："
                            android:textColor="#ff333333"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/tv_with_draw_status"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="提现中"
                            android:textColor="#ffce2626"
                            android:textSize="15sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tv_witdh_status_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_14"
                        android:text="2020年9月11日"
                        android:textColor="#ffce2626"
                        android:textSize="14sp" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp_0_5"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginRight="@dimen/dp_12"
                    android:background="#F6F6F6" />


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_12"
                    android:layout_marginTop="@dimen/dp_15"
                    android:layout_marginBottom="@dimen/dp_30"
                    android:text="提现进程："
                    android:textColor="#ff333333"
                    android:textSize="15sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_25"
                        android:layout_marginRight="@dimen/dp_25">

                        <TextView
                            android:id="@+id/tv_refund_money"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="发起提现"
                            android:textColor="#ff4795ed"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_shenhe"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_91"
                            android:layout_toRightOf="@id/tv_refund_money"
                            android:text="提现中"
                            android:textColor="#ff4795ed"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_refund_success"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_93"
                            android:layout_toRightOf="@id/tv_shenhe"
                            android:text="提现成功"
                            android:textColor="#ffcccccc"
                            android:textSize="12sp" />

                        <View
                            android:id="@+id/view_one"
                            android:layout_width="@dimen/dp_5"
                            android:layout_height="@dimen/dp_5"
                            android:layout_below="@id/tv_refund_money"
                            android:layout_marginLeft="@dimen/dp_22"
                            android:layout_marginTop="@dimen/dp_15"
                            android:background="@drawable/shape_blue_circle_10" />

                        <View
                            android:id="@+id/view_divider_one"
                            android:layout_width="@dimen/dp_114"
                            android:layout_height="@dimen/dp_1"
                            android:layout_below="@id/tv_refund_money"
                            android:layout_marginLeft="@dimen/dp_4"
                            android:layout_marginTop="@dimen/dp_17"
                            android:layout_marginRight="@dimen/dp_4"
                            android:layout_toRightOf="@id/view_one"
                            android:background="#4795ED" />

                        <View
                            android:id="@+id/view_two"
                            android:layout_width="@dimen/dp_5"
                            android:layout_height="@dimen/dp_5"
                            android:layout_below="@id/tv_shenhe"
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_toRightOf="@id/view_divider_one"
                            android:background="@drawable/shape_blue_circle_10" />


                        <View
                            android:id="@+id/view_divider_two"
                            android:layout_width="@dimen/dp_114"
                            android:layout_height="@dimen/dp_1"
                            android:layout_below="@id/tv_refund_money"
                            android:layout_marginLeft="@dimen/dp_4"
                            android:layout_marginTop="@dimen/dp_17"
                            android:layout_marginRight="@dimen/dp_4"
                            android:layout_toRightOf="@id/view_two"
                            android:background="#4795ED" />

                        <View
                            android:id="@+id/view_three"
                            android:layout_width="@dimen/dp_5"
                            android:layout_height="@dimen/dp_5"
                            android:layout_below="@id/tv_shenhe"
                            android:layout_marginLeft="@dimen/dp_5"
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_toRightOf="@id/view_divider_two"
                            android:background="@drawable/shape_gray_circle_10" />


                        <TextView
                            android:id="@+id/tv_time_one"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/view_divider_one"
                            android:layout_marginTop="@dimen/dp_15"
                            android:text="2020-09-10 13:00"
                            android:textColor="#ff333333"
                            android:textSize="12sp" />

                        <TextView
                            android:id="@+id/tv_time_two"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/view_divider_two"
                            android:layout_marginLeft="@dimen/dp_30"
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_toRightOf="@id/tv_time_one"
                            android:text="2020-09-10 13:00"
                            android:textColor="#ff333333"
                            android:textSize="12sp" />
                        <TextView
                            android:id="@+id/tv_time_three"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/view_divider_two"
                            android:layout_marginLeft="@dimen/dp_30"
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_toRightOf="@id/tv_time_two"
                            android:text="2020-09-10 13:00"
                            android:textColor="#ff333333"
                            android:textSize="12sp" />
                    </RelativeLayout>

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/dp_1"
                        android:layout_marginLeft="@dimen/dp_12"
                        android:layout_marginTop="@dimen/dp_15"
                        android:layout_marginRight="@dimen/dp_12"
                        android:background="#F6F6F6" />

                    <LinearLayout
                        android:id="@+id/ll_refund_message"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginTop="@dimen/dp_15"
                            android:text="提现金额："
                            android:textColor="#ff333333"
                            android:textSize="15sp" />

                        <TextView
                            android:id="@+id/tv_with_draw_money"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_marginBottom="@dimen/dp_15"
                            android:text="¥40"
                            android:textColor="#ff999999"
                            android:textSize="14sp" />

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_0_5"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginRight="@dimen/dp_12"
                            android:background="#F6F6F6" />
                        <TextView
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="申请时间："
                            android:textColor="#ff333333"
                            android:textSize="15sp"
                            />
                        <TextView
                            android:id="@+id/tv_apply_time"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="2020-09-11  13:00"
                            android:textColor="#ff999999"
                            android:textSize="14sp"
                            />
                        <View
                            android:layout_marginTop="@dimen/dp_15"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_0_5"
                            android:layout_marginLeft="@dimen/dp_12"
                            android:layout_marginRight="@dimen/dp_12"
                            android:background="#F6F6F6" />
                        <TextView
                            android:layout_marginTop="@dimen/dp_17"
                            android:layout_marginLeft="@dimen/dp_26"
                            android:layout_marginRight="@dimen/dp_26"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="您的赏金会在自提现发起之日起3天内到账，请耐
心等待！"
                            android:textColor="#ff666666"
                            android:textSize="15sp"
                            />
                    </LinearLayout>


                </LinearLayout>


            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </LinearLayout>
</layout>