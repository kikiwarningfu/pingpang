                                android:layout_height="@dimen/dp_1"
                                android:layout_marginLeft="@dimen/dp_12"
                                android:layout_marginRight="@dimen/dp_12"
                                android:background="@color/color_f5" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/dp_42"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/tv_taishu"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentRight="true"
                                    android:layout_gravity="center"
                                    android:layout_marginLeft="@dimen/dp_12"
                                    android:layout_marginRight="@dimen/dp_3"
                                    android:gravity="center"
                                    android:paddingTop="@dimen/dp_3"
                                    android:paddingBottom="@dimen/dp_3"
                                    android:text="名次"
                                    android:textColor="#FF333333"
                                    android:textSize="14sp" />

                                <FrameLayout
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1">

                                    <androidx.recyclerview.widget.RecyclerView
                                        android:id="@+id/rv_ranking"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent">

                                    </androidx.recyclerview.widget.RecyclerView>

                                    <RelativeLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:orientation="horizontal">

                                        <View
                                            android:id="@+id/view_left"
                                            android:layout_width="@dimen/dp_20"
                                            android:layout_height="match_parent"
                                            android:background="@drawable/shape_gradient_music_player_left">

                                        </View>


                                        <View
                                            android:id="@+id/view_right"
                                            android:layout_width="@dimen/dp_20"
                                            android:layout_height="match_parent"
                                            android:layout_alignParentRight="true"
                                            android:background="@drawable/shape_gradient_music_player">

                                        </View>
                                    </RelativeLayout>
                                </FrameLayout>

                            </LinearLayout>
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_40"
                            android:layout_marginLeft="@dimen/dp_37"
                            android:layout_marginTop="@dimen/dp_10"
                            android:layout_marginRight="@dimen/dp_37"

                            android:layout_marginBottom="@dimen/dp_10"
                            android:background="@drawable/shape_game_detail_search"
                            android:visibility="gone">

                            <ImageView
                                android:layout_width="@dimen/dp_26"
                                android:layout_height="@dimen/dp_26"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/dp_12"
                                android:src="@drawable/icon_club_search" />

                            <EditText
                                android:id="@+id/et_search_sports"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_marginLeft="@dimen/dp_51"
                                android:background="@null"
                                android:hint="输入运动员名字搜索"
                                android:imeOptions="actionSearch"
                                android:inputType="text"
                                android:textColor="@color/black"
                                android:textColorHint="#999999">

                            </EditText>

                        </RelativeLayout>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/dp_10"
                            android:background="@color/color_f5" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>



                <androidx.core.widget.NestedScrollView
                android:id="@+id/scrollview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fillViewport="true">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@color/white"
                            android:orientation="vertical">

                            <!--出组名单-->
                            <LinearLayout
                                android:id="@+id/ll_first_stage"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:visibility="gone">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/dp_17"
                                    android:orientation="horizontal">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1">

                                        <ImageView
                                            android:layout_width="@dimen/dp_17"
                                            android:layout_height="@dimen/dp_19"
                                            android:layout_marginLeft="@dimen/dp_11"
                                            android:layout_marginTop="@dimen/dp_11"
                                            android:src="@drawable/img_play_by_play" />

                                        <TextView
                                            android:id="@+id/tv_first_stage_title"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginLeft="@dimen/dp_4"
                                            android:layout_marginTop="@dimen/dp_13"
                                            android:text="超级组男子单打  第一阶段比赛详情"
                                            android:textColor="#fffdb700"
                                            android:textSize="11sp" />
                                    </LinearLayout>


                                </LinearLayout>

                                <include layout="@layout/activity_hvform" />
                            </LinearLayout>
                            <!--第二阶段 淘汰赛-->
                            <LinearLayout
                                android:id="@+id/ll_elimination_detail"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:visibility="gone">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:gravity="center_vertical"
                                    android:orientation="horizontal"
                                    android:paddingLeft="@dimen/dp_12"
                                    android:paddingTop="@dimen/dp_14"
                                    android:paddingBottom="@dimen/dp_14">

                                    <ImageView
                                        android:layout_width="@dimen/dp_14"
                                        android:layout_height="@dimen/dp_14"
                                        android:src="@drawable/img_game_detail_title" />

                                    <TextView
                                        android:id="@+id/tv_elimination_detail_title"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginLeft="@dimen/dp_10"
                                        android:text=""
                                        android:textColor="#fffdb700"
                                        android:textSize="12sp" />
                                </LinearLayout>


                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/rv_eleminate_match"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/dp_12"
                                    android:layout_marginRight="@dimen/dp_12"
                                    android:layout_marginBottom="@dimen/dp_10" />
                            </LinearLayout>
                            <!--循环赛 第一阶段-->
