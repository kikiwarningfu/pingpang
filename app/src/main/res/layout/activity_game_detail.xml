                                    android:orientation="vertical"
                                    android:padding="1.5dp">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/dp_30"
                                        android:background="#fff6f6f6"
                                        android:orientation="horizontal">

