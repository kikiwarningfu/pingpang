                                            android:layout_height="@dimen/dp_9"
                                            android:layout_marginTop="@dimen/dp_11"
                                            android:layout_marginRight="@dimen/dp_13"
                                            android:src="@drawable/img_yellow_arrow_down" />
                                    </LinearLayout>


                                </LinearLayout>

                                <include

                                    layout="@layout/activity_group_list" />

                            </LinearLayout>
                            <!--成绩单-->
                            <LinearLayout
